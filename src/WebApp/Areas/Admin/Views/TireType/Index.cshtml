@model IEnumerable<Domain.Entities.TireType>

@{
    ViewData["Title"] = "Tekerlek Tipi İşlemleri";
}

<h1>Tekerlek Tipi İşlemleri</h1>

<p>
    <a asp-action="Create">Yeni Kayıt</a>
</p>
<table class="table">
    <thead>
        <tr>
           
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Düzenle", "Edit", new { id=item.Id }) |
                @Html.ActionLink("Sil", "Delete", new { id=item.Id })
            </td>
        </tr>
}
    </tbody>
</table>
